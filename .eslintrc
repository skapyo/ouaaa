{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
      // "semi": "error",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/react-in-jsx-scope": "off",
      "react/prop-types":1,
      // "eslint/no-console": ["warn", { "allow": ["warn", "error"] }],
      "no-console": "off"
  },
  "plugins": [
    "react-hooks"
  ],
  "extends": [
    "plugin:react/recommended",
    "eslint:recommended"  
  ]
}